
<button type="button" class="btn btn-outline-success" id="edit" data-id="{{blogData.id}}">Edit</button>
<button type="button" class="btn btn-outline-danger" id="delete" data-id="{{blogData.id}}">Delete</button>
{{blogData.title}} 
{{blogData.description}}
{{#each blogData.comments as |comment|}}
  <div>
    <p>Description: {{comment.description}}</p>
    <p>Date Created: {{format_date comment.date_created}}</p>
    <p>Commented by: {{comment.user.name}}</p>
  </div>
{{/each}}
<script src="/js/dashboard.js"></script>


